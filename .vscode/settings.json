{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "rust-analyzer.checkOnSave.command": "clippy",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer", // Makes the magic
    "editor.formatOnSave": true // Optional
  },
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "zsh",
      "args": ["-i"]
    }
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    },
    "Storybook": {
      "id": "storybook",
      "extensionIdentifier": "chaosreactor.storybook",
      "path": "zsh",
      "args": ["-c", "nx storybook ui"],
      "overrideName": true,
      "source": "Storybook",
      "focus": false,
      "onlySingle": true,
      "persistent": "storybook",
      "command": "nx storybook ui",
      "icon": "book",
      "color": "terminal.ansiMagenta"
    }
  },
  "peacock.remoteColor": "#42b883",
  "sqltools.connections": [
    {
      "mysqlOptions": {
        "authProtocol": "default"
      },
      "previewLimit": 50,
      "server": "localhost",
      "port": 3306,
      "driver": "MySQL",
      "name": "Chaos Reactor desktop",
      "database": "reactor",
      "username": "root",
      "password": ""
    }
  ]
}
